<Window x:Class="HashTableApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HashTableApp"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="addBtn" Content="Add" HorizontalAlignment="Left" Height="46" Margin="176,256,0,0" VerticalAlignment="Top" Width="104" Click="addBtn_Click"/>
        <Button x:Name="removeBtn" Content="Remove" HorizontalAlignment="Left" Height="46" Margin="320,256,0,0" VerticalAlignment="Top" Width="104" Click="removeBtn_Click"/>
        <Button x:Name="showBtn" Content="Show" HorizontalAlignment="Left" Height="46" Margin="454,256,0,0" VerticalAlignment="Top" Width="104" Click="showBtn_Click"/>
        <TextBox x:Name="txtKey" HorizontalAlignment="Left" Height="43" Margin="176,142,0,0" TextWrapping="Wrap" Text="Key" VerticalAlignment="Top" Width="382"/>
        <TextBox x:Name="txtValue" HorizontalAlignment="Left" Height="43" Margin="176,0,0,0" TextWrapping="Wrap" Text="Value" VerticalAlignment="Center" Width="382"/>

    </Grid>
</Window>
